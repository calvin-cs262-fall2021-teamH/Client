{
  "expo": {
    "name": "hello-campus-d1",
    "slug": "hello-campus-d1",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/HelloCampusLogo_NoBackground.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationUsageDescription": "This app uses location to show the user's location on a map",
        "UIBackgroundModes": [
          "location"
        ]
      }
    },
    "android": {
      "package": "edu.calvin.cs262huh",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "LOCATION",
        "ACCESS_BACKGROUND_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/HelloCampusLogo_NoBackground.png"
    },
    "description": ""
  }
}
